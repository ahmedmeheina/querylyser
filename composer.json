{
    "name": "ameheina/querylyser",
    "description": "Analyse laravel queries ",
    "keywords": [
        "ameheina",
        "querylyser"
    ],
    "homepage": "https://github.com/ameheina/querylyser",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ahmed Meheina",
            "email": "ahmedmeheina@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Ameheina\\Querylyser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ameheina\\Querylyser\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ameheina\\Querylyser\\QuerylyserServiceProvider"
            ],
            "aliases": {
                "Querylyser": "Ameheina\\Querylyser\\QuerylyserFacade"
            }
        }
    }
}
